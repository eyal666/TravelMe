@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title</h2>
<h3>@ViewBag.Subtitle</h3>

<div class="container">
    <div>
        <video autoplay="autoplay" width="1000px" height="450px" and loop>
            <source src="~/Content/Media/video.mp4" type="video/mp4" />
            Your browser does not support the video tag.
        </video>
    </div>
    <div class="divider">
        <p>@ViewBag.Description</p>
    </div>
</div>
