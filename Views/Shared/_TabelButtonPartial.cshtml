@model TravelMe.Models.IndividualButtonPartial
@using TravelMe.Models

<td style="width:150px">
    <div role="group">
        @Html.Partial("_IndividualButtonPartial",
        new IndividualButtonPartial
        {
           Action = "Edit",
           ButtonType = "btn-primary",
           Glyph = "pencil",
           Text = "Edit",
           GenreId = Model.GenreId,
           BookId = Model.BookId,
           CustomerId = Model.CustomerId,
           MembershipTypeId = Model.MembershipTypeId,
           userId = Model.userId
        })
        @Html.Partial("_IndividualButtonPartial",
        new IndividualButtonPartial
        {
           Action = "Details",
           ButtonType = "btn-success",
           Glyph = "list",
           Text = "Details",
           GenreId = Model.GenreId,
           BookId = Model.BookId,
           CustomerId = Model.CustomerId,
           MembershipTypeId = Model.MembershipTypeId,
           userId = Model.userId
        })
        @Html.Partial("_IndividualButtonPartial",
        new IndividualButtonPartial
        {
           Action = "Delete",
           ButtonType = "btn-danger",
           Glyph = "trash",
           Text = "Delete",
           GenreId = Model.GenreId,
           BookId = Model.BookId,
           CustomerId = Model.CustomerId,
           MembershipTypeId = Model.MembershipTypeId,
           userId = Model.userId
        })
    </div>
</td>

